<div class="report compact">
  <table class="report-table">
    <thead>
      <tr>
        <th class="rowh">項目</th>
        <% @employees.each do |emp| %>
          <th class="empcol">
            <div class="emphead">
              <span class="code"><%= emp.code %></span>
              <span class="name"><%= emp.name %></span>
            </div>
          </th>
        <% end %>
      </tr>
    </thead>
    <tbody>
      <% @items.each do |item| %>
        <tr>
          <th class="rowh"><%= item.name %></th>
          <% @employees.each do |emp| %>
            <% cell = @cell_by[[emp.id, item.id]] %>
            <td class="<%= item.above_basic ? 'num time' : 'num money' %>">
              <%= display_cell(cell, item: item) %>
            </td>
          <% end %>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
<%= render partial: "table_style" %>
